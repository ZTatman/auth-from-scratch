// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

model User {
  id       String   @id @default(uuid())
  username String   @unique
  password String
  createAt DateTime @default(now())
}
